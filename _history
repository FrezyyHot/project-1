put.running_time()\n    hazardTimer = 0\n    sumLight = 0\n    readingCount = 0\n    basic.show_icon(IconNames.YES)\n    # Brief pause to show start\n    basic.pause(500)\ninput.on_button_pressed(Button.A, on_button_pressed_a)\n\n# 3. Button B: Displays Average Light Level\n\ndef on_button_pressed_b():\n    global average\n    if readingCount > 0:\n        average = Math.round(sumLight / readingCount)\n        basic.show_number(average)\n    else:\n        basic.show_string(\"No Data\")\ninput.on_button_pressed(Button.B, on_button_pressed_b)\n\ncurrentLight = 0\naverage = 0\nreadingCount = 0\nsumLight = 0\nhazardTimer = 0\nstartTime = 0\ntestRunning = False\n# 1. Setup on Start\nbasic.show_icon(IconNames.HAPPY)\n# 4. Data Logging Loop: Runs every 2 seconds [cite: 33]\n\ndef on_every_interval():\n    global currentLight, sumLight, readingCount\n    if testRunning:\n        currentLight = input.light_level()\n        # Add to average calculation\n        sumLight += currentLight\n        readingCount += 1\n        # Send data to Excel (Data Streamer)\n        # Logs Test Number, Time elapsed (seconds), and Light Level\n        serial.write_line(\"\" + str(Math.round((input.running_time() - startTime) / 1000)) + \",\" + str(currentLight))\nloops.every_interval(2000, on_every_interval)\n\n# 5. Main Loop: Safety Logic and 1-Minute Timer\n\ndef on_forever():\n    print(input.running_time())\n    global currentLight, testRunning, hazardTimer\n    if testRunning:\n        currentLight = input.light_level()\n        # CHECK 1: Has 1 minute passed? [cite: 33]\n        if input.running_time() - startTime > 60000:\n            testRunning = False\n            # Test Complete\n            basic.show_icon(IconNames.HAPPY)\n        else:\n            # CHECK 2: Hazard Logic (Light < 25 for > 10s)\n            if currentLight < 25:\n                # If hazard timer hasn't started, start it now\n                if hazardTimer == 0:\n                    hazardTimer = input.running_time()\n                # If timer has been running for > 10 seconds\n                if input.running_time() - hazardTimer > 10000:\n                    # DESTROYED\n                    basic.show_icon(IconNames.SKULL)\n                    # Reset all timers and counters for a new test\n                    testRunning = False\n                    basic.pause(2000)\n                else:\n                    # Warning state\n                    basic.show_icon(IconNames.CONFUSED)\n            else:\n                # Light is safe, reset hazard timer\n                hazardTimer = 0\n                # Safe state\n                basic.show_icon(IconNames.HEART)\n    else:\n        # Test Complete\n        basic.show_icon(IconNames.HAPPY)\nbasic.forever(on_forever)\n","pxt.json":"{\n    \"name\": \"Project 1\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\",\n        \"main.py\"\n    ],\n    \"preferredEditor\": \"pyprj\"\n}\n"}},{"timestamp":1770650061156,"editorVersion":"8.0.18","text":{"main.blocks":"<xml xmlns=\"https://developers.google.com/blockly/xml\"><variables><variable id=\"`d0[(K=A#]aInKy}R}/=\">testRunning</variable><variable id=\"A`:qXNIh3in4RJ/FT1MS\">startTime</variable><variable id=\"b@aPsMCc(w`FZX2%GV+1\">hazardTimer</variable><variable id=\"B!l^y7]G+8W5%cD7W;y{\">sumLight</variable><variable id=\"OsJl6pnq0_y8By2vwrTB\">readingCount</variable><variable id=\"v)Xrw/rxh`n$;#0,^By$\">average</variable><variable id=\"f{p-LbOBf3:?!rsoFP{`\">currentLight</variable></variables><block type=\"pxt-on-start\" x=\"0\" y=\"0\"><statement name=\"HANDLER\"><block type=\"basic_show_icon\"><field name=\"i\">IconNames.Happy</field></block></statement></block><block type=\"device_button_event\" x=\"272\" y=\"0\"><field name=\"NAME\">Button.B</field><statement name=\"HANDLER\"><block typ